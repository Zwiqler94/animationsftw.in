{"a":"import {transition,\n trigger,\n group,\n animate,\n style,\n query} from '@angular/animations';\n@Component({  animations: [\n    trigger('routeAnimations',\n [\n      transition('homePage => profilePage',\n [\n        group([\n          query(':enter',\n style({ opacity: 0 })),\n          query(':leave',\n [\n            animate('1s',\n style({ opacity: 0 }))          ])\n,\n          query(':enter',\n [\n            animate('1s',\n style({ opacity: 1 }))          ])\n,\n        ])\n      ])\n    ])\n  ]}) \n class AppComponent {}"}